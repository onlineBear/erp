<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zmqy.erp.mapper.mis.pc.component.sys.base.UserCpmMapper">

    <select id="getUserResourceUrlList" parameterType="map" resultType="String">
        select r.resUrl
        from tUserResource as ur
        join tResource as r on ur.resId=r.id
        where ur.userId = #{userId}
        order by r.id
    </select>
</mapper>
